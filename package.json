{
  "name": "redis-cluster-ui",
  "version": "0.1.0",
  "description": "An FrontEnd to easy Redis Cluster management",
  "main": "main.js",
  "dependencies": {
    "electron": "^3.0.12",
    "ioredis": "^4.3.0",
    "lodash": "^4.17.11"
  },
  "devDependencies": {
    "electron-reloader": "^0.2.0",
    "husky": "^1.2.1",
    "lint-staged": "^8.1.0",
    "standard": "^12.0.1"
  },
  "scripts": {
    "start": "electron ."
  },
  "keywords": [
    "Electron",
    "Redis"
  ],
  "author": "zhongchiyu@gmail.com",
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "standard --fix",
      "git add"
    ]
  },
  "standard": {
    "globals": [
      "d3",
      "$"
    ]
  }
